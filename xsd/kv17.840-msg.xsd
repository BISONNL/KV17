<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tmi8="http://bison.connekt.nl/tmi8/kv17/msg" xmlns:tmi8c="http://bison.connekt.nl/tmi8/kv17/core" targetNamespace="http://bison.connekt.nl/tmi8/kv17/msg" elementFormDefault="qualified">
	<xs:import namespace="http://bison.connekt.nl/tmi8/kv17/core" schemaLocation="kv17-core.xsd"/>
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- TMI KV17 messages                                                                 -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<xs:element name="VV_TM_PUSH">
		<xs:annotation>
			<xs:documentation>Bevat door de integrator te verwerken mutaties op de dagplanning</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="tmi8:MessageProperties"/>
				<xs:element name="KV17cvlinfo" type="tmi8:KV17cvlinfoType" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="VV_TM_REQ">
		<xs:annotation>
			<xs:documentation>Vraagt de leverancier mutaties opnieuw te versturen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="tmi8:MessageProperties"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="VV_TM_RES">
		<xs:annotation>
			<xs:documentation>Geeft aan hoe de aan de integrator aangeboden documenten verwerkt zijn</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="tmi8:MessageProperties" minOccurs="0"/>
				<xs:element name="ResponseCode" type="tmi8:ResponseCodeType"/>
				<xs:element name="ResponseError" type="tmi8:ResponseErrorType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- Dossiers: KV17cvlinfo                                                             -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<xs:complexType name="KV17cvlinfoType">
		<xs:sequence>
			<xs:element name="KV17JOURNEY" type="tmi8:KV17journeyType"/>
			<xs:element name="KV17MUTATEJOURNEY" type="tmi8:KV17mutatejourneyType" minOccurs="0"/>
			<xs:element name="KV17MUTATEJOURNEYSTOP" type="tmi8:KV17mutatejourneystopType" minOccurs="0"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KV17journeyType">
		<xs:sequence>
			<xs:element name="dataownercode" type="tmi8:dataownercodeType"/>
			<xs:choice>
				<xs:group ref="tmi8:oneJourneyGroup"/>
				<xs:group ref="tmi8:allJourneysOfLineGroup"/>
				<xs:group ref="tmi8:allLinesGroup"/>
			</xs:choice>
			<xs:element name="begintime" type="tmi8:tmitimeType" minOccurs="0"/>
			<xs:element name="endtime" type="tmi8:tmitimeType" minOccurs="0"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:group name="oneJourneyGroup">
		<!-- Individuele rit -->
		<xs:sequence>
			<xs:element name="lineplanningnumber" type="tmi8:lineplanningnumberType"/>
			<xs:element name="operatingday" type="tmi8:tmidateType"/>
			<xs:element name="journeynumber" type="tmi8:journeynumberType"/>
			<xs:element name="reinforcementnumber" type="tmi8:reinforcementnumberType"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="allJourneysOfLineGroup">
		<!-- Alle ritten van een specifieke lijn -->
		<xs:sequence>
			<xs:element name="allJourneysOfLine"/>
			<xs:element name="lineplanningnumber" type="tmi8:lineplanningnumberType"/>
			<xs:element name="operatingday" type="tmi8:tmidateType"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="allLinesGroup">
		<!-- Alle ritten van alle lijnen van een vervoerder -->
		<xs:sequence>
			<xs:element name="allLines"/>
			<xs:element name="operatingday" type="tmi8:tmidateType"/>
		</xs:sequence>
	</xs:group>
	<xs:complexType name="KV17mutatejourneyType">
		<xs:annotation>
			<xs:documentation>ADD is a RESERVED OPERATION</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="timestamp" type="tmi8:tmidatetimeType"/>
			<xs:choice>
				<xs:element name="CANCEL" type="tmi8:CANCELType"/>
				<xs:element name="RECOVER" type="tmi8:RECOVERType"/>
				<xs:element name="ADD" type="tmi8:ADDType"/>
				<xs:element name="NOTMONITORED" type="tmi8:NOTMONITOREDType"/>
			</xs:choice>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KV17mutatejourneystopType">
		<xs:sequence>
			<xs:element name="timestamp" type="tmi8:tmidatetimeType"/>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="SHORTEN" type="tmi8:SHORTENType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="CHANGEPASSTIMES" type="tmi8:CHANGEPASSTIMESType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="CHANGEDESTINATION" type="tmi8:CHANGEDESTINATIONType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="LAG" type="tmi8:LAGType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="MUTATIONMESSAGE" type="tmi8:MUTATIONMESSAGEType" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- CVL Info entities                                                                 -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<xs:complexType name="CANCELType">
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element name="reasontype" type="tmi8:sirisxcategoryType"/>
				<xs:element name="subreasontype" type="tmi8:sirisxcodeType"/>
			</xs:sequence>
			<xs:element name="reasoncontent" type="tmi8:contentType" minOccurs="0"/>
			<xs:sequence minOccurs="0">
				<xs:element name="advicetype" type="tmi8:sirisxcategoryType"/>
				<xs:element name="subadvicetype" type="tmi8:sirisxcodeType"/>
			</xs:sequence>
			<xs:element name="advicecontent" type="tmi8:contentType" minOccurs="0"/>
			<xs:element name="showcancelledtrip" type="tmi8:showcancelledtripType" default="true" minOccurs="0"/>
			<xs:element name="autorecover" type="xs:boolean" default="false" minOccurs="0"/>
			<xs:element name="alertcause" type="tmi8:AlertCauseEnumeration" default="unknown" minOccurs="0"/>
			<xs:element name="servicecondition" type="tmi8:ServiceConditionEnumeration" default="unknown" minOccurs="0"/>
			<xs:element name="serviceref" type="xs:string" default="false" minOccurs="0"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SHORTENType">
		<xs:sequence>
			<xs:element name="userstopcode" type="tmi8:codeType"/>
			<xs:element name="passagesequencenumber" type="tmi8:passagesequencenumberType"/>
			<xs:element name="showcancelledtrip" type="tmi8:showcancelledtripType" default="true" minOccurs="0"/>
			<xs:element name="alertcause" type="tmi8:AlertCauseEnumeration" default="unknown" minOccurs="0"/>
			<xs:element name="servicecondition" type="tmi8:ServiceConditionEnumeration" default="unknown" minOccurs="0"/>
			<xs:element name="serviceref" type="xs:string" default="false" minOccurs="0"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RECOVERType">
		<xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ADDType">
		<xs:annotation>
			<xs:documentation>ADD is a RESERVED OPERATION</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<!-- Mogelijke oplossing voor CVL rit toevoegen ingreep. NIET GEBRUIKEN !!!
      <xs:choice>
        <xs:element name="insertfromscratch" type="tmi8:tmibooleanType"/>
        <xs:element name="COPYFROMJOURNEY" type="tmi8:KV17journeyType"/>
      </xs:choice>
-->
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NOTMONITOREDType">
		<xs:sequence>
			<xs:element name="monitoringerror" type="tmi8:monitoringerrorType" minOccurs="0"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LAGType">
		<xs:sequence>
			<xs:element name="userstopcode" type="tmi8:codeType"/>
			<xs:element name="passagesequencenumber" type="tmi8:passagesequencenumberType"/>
			<xs:element name="lagtime" type="tmi8:lagtimeType"/>
			<xs:element name="alertcause" type="tmi8:AlertCauseEnumeration" default="unknown" minOccurs="0"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CHANGEPASSTIMESType">
		<xs:sequence>
			<xs:element name="userstopcode" type="tmi8:codeType"/>
			<xs:element name="passagesequencenumber" type="tmi8:passagesequencenumberType"/>
			<xs:element name="targetarrivaltime" type="tmi8:tmitimeType"/>
			<xs:element name="targetdeparturetime" type="tmi8:tmitimeType"/>
			<xs:element name="journeystoptype" type="tmi8:journeystoptypeType"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CHANGEDESTINATIONType">
		<xs:sequence>
			<xs:element name="userstopcode" type="tmi8:codeType"/>
			<xs:element name="passagesequencenumber" type="tmi8:passagesequencenumberType"/>
			<xs:element name="destinationcode" type="tmi8:codeType" minOccurs="0"/>
			<xs:element name="destinationname50" type="tmi8:destinationname50Type"/>
			<xs:element name="destinationname16" type="tmi8:destinationname16Type"/>
			<xs:element name="destinationdetail16" type="tmi8:destinationdetail16Type" minOccurs="0"/>
			<xs:element name="destinationdisplay16" type="tmi8:destinationdisplay16Type" minOccurs="0"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MUTATIONMESSAGEType">
		<xs:sequence>
			<xs:element name="userstopcode" type="tmi8:codeType"/>
			<xs:element name="passagesequencenumber" type="tmi8:passagesequencenumberType"/>
			<xs:sequence minOccurs="0">
				<xs:element name="reasontype" type="tmi8:sirisxcategoryType"/>
				<xs:element name="subreasontype" type="tmi8:sirisxcodeType"/>
			</xs:sequence>
			<xs:element name="reasoncontent" type="tmi8:contentType" minOccurs="0"/>
			<xs:sequence minOccurs="0">
				<xs:element name="advicetype" type="tmi8:sirisxcategoryType"/>
				<xs:element name="subadvicetype" type="tmi8:sirisxcodeType"/>
			</xs:sequence>
			<xs:element name="advicecontent" type="tmi8:contentType" minOccurs="0"/>
			<xs:element name="showcancelledtrip" type="tmi8:showcancelledtripType" default="true" minOccurs="0"/>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="tmi8c:delimiter"/>
				<xs:any namespace="##targetNamespace ##local" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- Message property types                                                            -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<xs:group name="MessageProperties">
		<xs:sequence>
			<xs:element name="SubscriberID" type="tmi8:SubscriberIDType"/>
			<xs:element name="Version" type="tmi8:VersionType"/>
			<xs:element name="DossierName" type="tmi8:DossierNameType"/>
			<xs:element name="Timestamp" type="tmi8:TimestampType"/>
		</xs:sequence>
	</xs:group>
	<xs:simpleType name="DossierNameType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="KV17cvlinfo"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VersionType">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SubscriberIDType">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TimestampType">
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="ResponseCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="OK"/>
			<xs:enumeration value="NOK"/>
			<xs:enumeration value="SE"/>
			<xs:enumeration value="NA"/>
			<xs:enumeration value="PE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AlertCauseEnumeration">
		<xs:annotation>
			<xs:documentation>
				Waardes voor TPEG Pts38 - AlertCause.
				Alleen de waardes _0, _30, _43, _77, _83, _85, _98, _124, _125 en _127 hebben een beschreven betekenis in KV17.
				Overige waardes zijn enkel opgenomen voor compatibiliteit met SIRI-SX.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="unknown">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_0, unknown</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="technicalProblem">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_30, technical problem</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="breakDown">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_43, break down</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="accident">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_77, accident</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="collision">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_83, collision</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="poorWeather">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_85, poor weather</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fallenTree">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_98, fallen tree</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="staffSickness">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_124, staff sickness</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="staffAbsence">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_125, staff absence</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="previousDisturbances">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_127, previous disturbances</xs:documentation>
				</xs:annotation>
			</xs:enumeration>

			<!-- Overige waardes zijn enkel opgenomen voor compatibiliteit met SIRI-SX  -->

			<xs:enumeration value="securityAlert">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_1, security alert</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="emergencyServicesCall">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_2, emergency services call</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="policeActivity">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_3, police activity</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="policeOrder">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_4, police order</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fire">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_5, fire</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="cableFire">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_6, cable fire</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="smokeDetectedOnVehicle">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_7, smoke detected on vehicle</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fireAtStation">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_8, fire at the station</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fireRun">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_9, fire run</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fireBrigadeOrder">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_10, fire brigade order</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="explosion">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_11, explosion</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="explosionHazard">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_12, explosion hazard</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="bombDisposal">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_13, bomb disposal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="emergencyMedicalServices">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_14, emergency medical services</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="emergencyBrake">
				<xs:annotation>
					<xs:documentation>TPEG Pts38_15, emergency brake</xs:documentation>
				</xs:annotation>
